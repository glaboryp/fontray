        "axios": "^1.11.0",
        "concurrently": "^9.0.1",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-vue": "^10.4.0",
        "laravel-vite-plugin": "^2.0.0",
        "prettier": "^3.6.2",
        "tailwindcss": "^4.0.0",
        "vite": "^7.0.4"
    },
    "dependencies": {
        "@inertiajs/vue3": "^2.1.3"
    }
